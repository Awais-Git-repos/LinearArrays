var a = [6,4,5]
for(var i = 0; i < a.length; i++){
    for(var j = i; j < a.length; j++){
        if(a[i] > a[j]){
            var b = 0;
            b = a[i];
            a[i] = a[j]
            a[j] = b
        }
    }
}
for(var i = 0; i < a.length; i++){
    
    document.write(a[i]+"<br>");
}
for(var i = 0; i < 3; i++){
    for(var j = i; j < 3; j++){
        document.write(j);
    }
    document.write("<br>");
}
